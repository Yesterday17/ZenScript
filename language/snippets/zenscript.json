{
    "For":{
        "prefix": "for",
        "body":[
            "for ${1:num},${2:item} in ${3:array} {",
            "\t",
            "}"
        ],
        "description": "For element"
    },
    "If":{
        "prefix": "if",
        "body":[
            "if (${cond}) {",
            "\t${branch1}",
            "} else {",
            "\t${branch2}",
            "}"
        ],
        "description": "If element else element"
    },
    "Val":{
        "prefix": "val",
